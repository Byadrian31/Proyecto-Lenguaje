import java.util.Scanner;
import libreria.FNumDAW;

public class CuantosCapicua{

    public static void main(String[] args) { 

        int n=1000;
        int x ;
        int cont=0;
    
        do{
            n++;
            x = FNumDAW.voltea(n);
            

            if(x == n){
                cont ++;
                System.out.println(n);
            }
            
        }while( n <= 9999);
        
        System.out.println("Hay:"+ cont + " capicua");        
    }
}
